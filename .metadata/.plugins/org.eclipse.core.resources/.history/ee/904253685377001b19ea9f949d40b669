package com.RDP.school.entity;

public class days {
	private Long id;
	private String day;
	
	
	public days() {
		super();
	}


	public days(Long id, String day) {
		super();
		this.id = id;
		this.day = day;
	}


	public Long getId() {
		return id;
	}


	public void setId(Long id) {
		this.id = id;
	}


	public String getDay() {
		return day;
	}


	public void setDay(String day) {
		this.day = day;
	}
	
	

}
